{
  "version": 2,
  "builds": [
    {
      "src": "apps/excalidraw-frontend/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "apps/http-backend/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "apps/ws-backend/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/excalidraw/(.*)",
      "dest": "apps/excalidraw-frontend/$1"
    },
    {
      "src": "/web/(.*)",
      "dest": "apps/web/$1"
    },
    {
      "src": "/api/http/(.*)",
      "dest": "apps/http-backend/$1"
    },
    {
      "src": "/api/ws/(.*)",
      "dest": "apps/ws-backend/$1"
    }
  ]
}
